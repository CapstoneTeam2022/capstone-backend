// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Address {
  id      Int    @id @default(autoincrement())
  city    String
  subCity String
  zone    String
  woreda  String
  kebelle String
  street  String
  houseNo String
}
